{
  "name": "wpetestframework",
  "version": "2.0.0",
  "description": "WpeTestFramework",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack && cp ./src/index.html ./build/index.html ",
    "localserver": "node ./localserver.js",
    "cli": "concurrently \"npm run localserver\" \"node index\"",
    "browser": "concurrently \"npm run localserver\" \"npm run build\" \"./node_modules/.bin/http-server ./build -o\"",
    "frontend": "concurrently \"npm run localserver\" \"cd ./frontend && npm run build\"  & ./node_modules/.bin/http-server ./frontend/dist -o"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebPlatformForEmbedded/WPETestFramework.git"
  },
  "author": "Michiel van der Geest <hello@michielvandergeest.com>",
  "bugs": {
    "url": "https://github.com/WebPlatformForEmbedded/WPETestFramework/issues"
  },
  "homepage": "https://github.com/WebPlatformForEmbedded/WPETestFramework#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^4.1.0",
    "contra": "^1.9.4",
    "esm": "^3.2.15",
    "http-server": "^0.11.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  }
}
